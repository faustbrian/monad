{
    "name": "cline/monad",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require-dev": {
        "cline/php-cs-fixer": "^1.0",
        "ergebnis/composer-normalize": "^2.49@dev",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-type-coverage": "^3.0",
        "orchestra/testbench": "^10.6"
    },
    "autoload": {
        "psr-4": {
            "Cline\\Monad\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    }
}
